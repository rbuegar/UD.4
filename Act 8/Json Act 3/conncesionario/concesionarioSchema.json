{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "codigo": {
        "type": "string",
        "pattern": "^C\\d{3}$"
      },
      "marca": {
        "type": "string"
      },
      "modelo": {
        "type": "string"
      },
      "matricula": {
        "type": "string",
        "pattern": "^[A-Z]{3}\\d{3}$"
      },
      "potencia": {
        "type": "string"
      },
      "plazas": {
        "type": "string"
      },
      "puertas": {
        "type": "string"
      }
    },
    "required": ["codigo", "marca", "modelo", "matricula", "potencia", "plazas", "puertas"]
  }
  